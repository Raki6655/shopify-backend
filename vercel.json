{
	"version": 2,
	"headers": [
		{
			"source": "/post/get-cart-items/:id",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				}
			]
		}
	],
	"builds": [
		{
			"src": "index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "index.js"
		}
	]
}
